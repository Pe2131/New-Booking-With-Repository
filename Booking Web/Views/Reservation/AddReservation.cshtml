
@{
    ViewData["Title"] = "AddReservation";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="container-fluid">
    <div class="block-header">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <ul class="breadcrumb breadcrumb-style ">
                    <li class="breadcrumb-item">
                        <h4 class="page-title">Reservation</h4>
                    </li>
                    <li class="breadcrumb-item bcrumb-1">
                        <a href="/Reservation/">
                            <i class="fas fa-home"></i> Reservation
                        </a>
                    </li>
                    <li class="breadcrumb-item bcrumb-2">
                        <a href="javascript:void(0);">Booking Form</a>
                    </li>
                </ul>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="card" style="padding:20px;">
                            @if (TempData["Style"] != null)
                            {
                                <div class="@TempData["Style"]">
                                    @TempData["Message"]
                                </div>
                            }
                            <a href="/Reservation" class="btn btn-primary">Create New Reserv</a>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

